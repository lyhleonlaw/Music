bg-logo($url)
  background-image :url($url+"@2x.png")
  @media (-webkit-min-device-pixel-ratio:3),(min-device-pixel-ratio:3)
    background-image :url($url+"@3x.png")

none-warp()
  text-overflow :ellipsis
  overflow :hidden
  white-space :nowrap

extend-click()
  position :relative
  &:before
    content:''
    position :absolute
    left :-10px
    top: -10px
    bottom: -10px
    right : -10px
